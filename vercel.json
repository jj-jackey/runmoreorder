{
  "version": 2,
  "name": "autorder",
  "functions": {
    "api/index.js": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/api/example-email-list",
      "dest": "/api/example-email-list.js"
    },
    {
      "src": "/api/auth/check",
      "dest": "/api/auth/check.js"
    },
    {
      "src": "/api/auth/admin-login",
      "dest": "/api/auth/admin-login.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*\\.(js|css|html|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 